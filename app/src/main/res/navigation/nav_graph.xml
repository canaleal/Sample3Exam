<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.canaleal.sample3.ui.FirstFragment"
        android:label="@string/input_fragment"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.canaleal.sample3.ui.SecondFragment"
        android:label="@string/output_fragment_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
        <argument
            android:name="petter"
            app:argType="com.canaleal.sample3.domain.Pet" />


    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.canaleal.sample3.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <action
        android:id="@+id/action_global_to_about"
        app:destination="@id/aboutFragment" />
    <dialog
        android:id="@+id/aboutFragment"
        android:name="com.canaleal.sample3.ui.AboutFragment"
        android:label="AboutFragment" />
</navigation>